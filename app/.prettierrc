{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "never",
  "endOfLine": "lf",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": ["prettier-plugin-tailwindcss"]
}
